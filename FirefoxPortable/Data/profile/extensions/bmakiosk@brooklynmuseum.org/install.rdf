<?xml version="1.0"?>

<RDF xmlns="http://www.w3.org/1999/02/22-rdf-syntax-ns#"
     xmlns:em="http://www.mozilla.org/2004/em-rdf#">

  <Description about="urn:mozilla:install-manifest">
    <em:id>bmakiosk@brooklynmuseum.org</em:id>
    <em:version>1.86</em:version>

    <!-- Firefox -->
    <em:targetApplication>
      <Description>
        <em:id>{ec8030f7-c20a-464f-9b0e-13a3a9e97384}</em:id>
        <em:minVersion>1.5</em:minVersion>
        <em:maxVersion>3.5.*</em:maxVersion>
      </Description>
    </em:targetApplication>

    <!-- Front End MetaData -->
    <em:name>BMA Kiosk</em:name>
    <em:description>BMA Kiosk Browser</em:description>
    <em:creator>Mozdev Group, Inc. for Brooklynmuseum of Art</em:creator>

    <em:homepageURL>http://mozdevgroup.com/clients/bm/</em:homepageURL>

    <!-- Packages, Skins and Locales that this extension registers -->
    <em:file>
      <Description about="urn:mozilla:extension:file:bmakiosk.jar">
        <em:package>content/</em:package>
        <em:skin>skin/</em:skin>
        <em:locale>locale/en-US/</em:locale>
      </Description>
    </em:file>
  </Description>
</RDF>
